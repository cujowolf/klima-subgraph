{
  "name": "klimadao-user-carbon",
  "version": "0.0.1",
  "repository": "https://github.com/KlimaDAO/klima-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "rm -rf ./generated && graph codegen",
    "build": "graph build",
    "test": "graph test",
    "create-local": "graph create --node http://127.0.0.1:8020 klima-current-refi-holdings",
    "remove-local": "graph remove --node http://127.0.0.1:8020 klima-current-refi-holdings",
    "watch-local": "graph deploy --watch --debug --node http://127.0.0.1:8020/ --ipfs http://127.0.0.1:5001 klima-current-refi-holdings",
    "deploy-local": "graph deploy --node http://127.0.0.1:8020 --ipfs http://127.0.0.1:5001 klima-current-refi-holdings",
    "create-args": "graph create --node $npm_config_node klima-current-refi-holdings",
    "deploy-args": "graph deploy --ipfs $npm_config_ipfs --node $npm_config_node klima-current-refi-holdings",
    "deploy-version": "graph deploy --ipfs $npm_config_ipfs --node $npm_config_node --version-label $npm_config_label klimklima-refi-current-holdingsa-current-refi-holdings",
    "deploy-staging": "graph deploy --ipfs https://api.staging.thegraph.com/ipfs/ --node https://api.staging.thegraph.com/deploy/ klima-current-refi-holdings",
    "deploy-hosted": "yarn codegen && graph deploy --product hosted-service cujowolf/klima-refi-current-holdings",
    "deploy": "yarn codegen && graph deploy --product hosted-service klimadao/klima-current-refi-holdings"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.37.6",
    "@graphprotocol/graph-ts": "^0.29.1",
    "prettier": "^1.18.2"
  }
}
